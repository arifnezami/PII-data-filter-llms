{
  "manifest_version": 3,
  "name": "PII Data Remover",
  "version": "1.0",
  "permissions": [
      "storage",
      "activeTab",
      "background"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "commands": {
      "toggle-pii": {
          "suggested_key": {
              "default": "Ctrl+Shift+Y"
          },
          "description": "Toggle PII detection"
      }
  }
}
